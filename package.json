{
  "name": "joey-mckenzie-tech",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"bun:dev:content\" \"bun:dev:svelte\"",
    "dev:svelte": "vite dev",
    "build": "svelte-kit sync && contentlayer build && vite build",
    "build:content": "contentlayer build",
    "build:content-clear": "contentlayer build --clearCache",
    "preview": "vite preview",
    "dev:content": "contentlayer dev",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check . && eslint .",
    "fmt": "prettier --write --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "ci": "bun run build && bun run lint"
  },
  "lint-staged": {
    "**/*.{svelte,ts,js,json,md}": "bun run fmt"
  },
  "devDependencies": {
    "@iconify/svelte": "^3.1.4",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@sveltejs/kit": "^1.20.4",
    "@tailwindcss/typography": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "url": "^0.11.3",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "type": "module",
  "dependencies": {
    "bits-ui": "^0.9.0",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "lucide-svelte": "^0.292.0",
    "mode-watcher": "^0.0.7",
    "radix-icons-svelte": "^1.2.1",
    "rehype-highlight": "^7.0.0",
    "rehype-pretty-code": "^0.10.2",
    "shiki": "^0.14.5",
    "tailwind-merge": "^2.0.0",
    "tailwind-variants": "^0.1.18"
  }
}
