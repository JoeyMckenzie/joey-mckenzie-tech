{
  "name": "joey-mckenzie-tech",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.10.5",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "prettier --plugin prettier-plugin-tailwindcss --check . && eslint .",
