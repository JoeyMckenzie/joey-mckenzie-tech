{
    "private": true,
    "type": "module",
    "packageManager": "pnpm@8.10.5",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && vite build && vite build --ssr",
        "fmt": "prettier --write .",
        "check": "prettier --check --plugin prettier-plugin-tailwindcss ./resources/js",
        "fix": "pnpm run check && eslint --fix \"./resources/js/**/*.vue\"",
        "lint": "pnpm run check && eslint \"./resources/js/**/*.vue\""
    },
    "lint-staged": {
        "**/*.{vue,js,ts,json,yml,md}": "pnpm run fmt"
    },
    "devDependencies": {
        "@iconify/vue": "^4.1.1",
        "@inertiajs/vue3": "^1.0.0",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.10",
        "@typescript-eslint/parser": "^6.11.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/server-renderer": "^3.2.31",
        "autoprefixer": "^10.4.12",
        "axios": "^1.6.1",
        "eslint": "^8.53.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-vue": "^9.18.1",
        "laravel-vite-plugin": "^0.8.0",
        "postcss": "^8.4.18",
        "prettier": "^3.1.0",
        "prettier-plugin-tailwindcss": "^0.5.7",
        "tailwindcss": "^3.2.1",
        "typescript": "^5.0.2",
        "vite": "^4.0.0",
        "vue": "^3.2.41",
        "vue-eslint-parser": "^9.3.2",
        "vue-tsc": "^1.2.0"
    },
    "dependencies": {
        "@radix-icons/vue": "^1.0.0",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "lint-staged": "^15.1.0",
        "tailwind-merge": "^2.0.0",
        "tailwindcss-animate": "^1.0.7"
    }
}
