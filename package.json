{
  "name": "joeymckenzie.tech",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "tailwind": "prettier --plugin prettier-plugin-tailwindcss --write src/",
    "lint": "pnpm dlx @biomejs/biome lint src/",
    "fmt": "pnpm run tailwind && pnpm dlx @biomejs/biome format src/ --write",
    "check": "pnpm dlx @biomejs/biome check --apply src/",
    "generate": "drizzle-kit generate:sqlite",
    "migrate": "drizzle-kit push:sqlite"
  },
  "lint-staged": {
    "**/*.{js,json,ts,tsx}": "pnpm run fmt"
  },
  "dependencies": {
    "@libsql/client": "0.4.0-pre.7",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@vercel/analytics": "^1.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^3.2.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.3",
    "next": "14.0.4",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.12.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "drizzle-kit": "^0.20.10",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rehype-highlight": "^7.0.0",
    "rehype-pretty-code": "^0.12.3",
    "rehype-shikiji": "^0.9.18",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
