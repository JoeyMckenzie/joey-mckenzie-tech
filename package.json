{
  "name": "joey-mckenzie-tech",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@8.6.7",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build -o test",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install",
    "lint": "eslint .",
    "turbo": "turbo build lint",
    "format": "prettier --write --ignore-unknown .",
    "format:check": "prettier --check .",
    "format:watch": "pnpm exec onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}"
  },
  "lint-staged": {
    "**/*.{cjs,mjs,ts,svelte,astro,md,json,yaml}": "pnpm format",
    "**/*.rs": "cargo fmt -- --emit=files",
    "**/*.zig": "zig fmt"
  },
  "dependencies": {
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/rss": "^2.4.3",
    "@astrojs/sitemap": "^2.0.1",
    "@astrojs/svelte": "^2.2.0",
    "@astrojs/tailwind": "^3.1.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "astro": "^2.10.7",
    "astro-icon": "^0.8.1",
    "svelte": "^3.54.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-astro": "^0.27.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "turbo": "^1.10.7"
  }
}
